    <%= form_with(model: appoinment, local: true) do |form| %>
    <%= form.label :booked_date %>
    <%= form.text_field :booked_date ,class: "ajaxcall", id: "datepicker"%>

    <%= form.label :start_time %>
    <%= form.text_field :start_time , class: "timepicker ajaxcall" , id: "start_time" %>

    <%= form.label :end_time %>
    <%= form.text_field :end_time , class: "timepicker ajaxcall", id: 'end_time' %>

    <%= form.select :doctor_id, options_for_select(get_doctor, "Select Doctor"), {:prompt => '-- Select Doctor --'} %>

    <%= form.submit "Save", class: 'btn btn-success' %>
    <% end %>

